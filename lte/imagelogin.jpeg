$( document ).ready(function() {
    
  function getCookie(name) {
    const cookiesArray = document.cookie.split(';');
    for (let cookie of cookiesArray) {
        const [cookieName, cookieValue] = cookie.trim().split('=');
        if (cookieName === name) {
            return cookieValue;
        }
    }
    return "";
}

